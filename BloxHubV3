local function NYHX_fake_script()
	local script = Instance.new('LocalScript', DontClick)

	script.Parent.MouseButton1Click:connect(function()
	
		game:GetService("TeleportService"):Teleport("10479799188")
	
	end)
end
coroutine.wrap(NYHX_fake_script)()
local function BLFI_fake_script()
	local script = Instance.new('LocalScript', Close)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(BLFI_fake_script)()
